{
    "ssl": {
        "key": "/certificates/firo.key",
        "certificate": "/certificates/firo.crt",
        "root": "/certificates/RootCA.pem"
    },
    "turn": {
        "url": "<URL> {TURN_TOKEN}",
        "turnToken": "<TURN_TOKEN>",
        "apiToken": "<API_TOKEN>"
    }
    "mounts": [
        {
            "name": "static",
            "type": "SPA",
            "path": "/",
            "directory": "../dist"
        }
    ],
    "origins": [
        "http://localhost",
        "https://localhost",
        "http://localhost:80",
        "https://localhost:443"
    ]
}